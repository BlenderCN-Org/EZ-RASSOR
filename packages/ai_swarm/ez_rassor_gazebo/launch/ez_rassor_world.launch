<launch>	  

	<env name="GAZEBO_MODEL_PATH" value="$(find ez_rassor_description)" />
  	<env name="GAZEBO_RESOURCE_PATH" value="$(find ez_rassor_description)" />

	<include file="$(find gazebo_ros)/launch/empty_world.launch">	    
		<arg name="world_name" value="$(find ez_rassor_gazebo)/worlds/ez_rassor.world"/>	    
		<arg name="gui" value="true"/>	  
	</include>	

	<!-- Load the SDF into the ROS Parameter Server -->                               
	<param name="robot_sdf" textfile="$(find ez_rassor_description)/sdf/model.sdf" />        

	<!-- Spawn the ez_rassor in gazebo --> 
	<node name="sdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
	args="-sdf -model ez_rassor -file $(find ez_rassor_description)/sdf/model.sdf"/>
 
</launch>