<launch>	  
	<include file="$(find gazebo_ros)/launch/empty_world.launch">	    
		<arg name="world_name" value="$(find ez_rassor_gazebo)/worlds/ez_rassor.world"/>	    
		<arg name="gui" value="true"/>	  
	</include>	

	<!-- urdf xml robot description loaded on the Parameter Server, converting the xacro into a proper urdf file-->
	<param name="robot_description" command="$(find xacro)/xacro.py '$(find ez_rassor_description)/urdf/ez_rassor.xacro'" />

	<!-- push robot_description to factory and spawn robot in gazebo -->
	<node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -model ez_rassor" />
 
</launch>