<?xml version="1.0" encoding="UTF-8"?>
<!-- Launch the request switch. Written by Tiger Sachse. -->
<launch>

  <arg name="id"
       default="1"
       doc="Robot id number."
  />
  
  <node name="request_switch"
        pkg="ezrassor_request_switch"
        type="request_switch"
        output="screen"
        ns="ezrassor$(arg id)"
  />
</launch>
