<?xml version="1.0" encoding="UTF-8"?>
<!-- Launch the topic switch. Written by Tiger Sachse. -->
<launch>
  <group ns="ezrassor1">
    <include file="$(find ezrassor_topic_switch)/launch/topic_switch.launch"/>
  </group>
  <group ns="ezrassor2">
    <include file="$(find ezrassor_topic_switch)/launch/topic_switch.launch">
      <arg name="primary_topic" value="primary2"/>
    </include>
  </group>

  <!--
  <arg name="primary_topic" default="primary_topic" doc="The switch's primary topic."/>
  <arg name="secondary_topic" default="secondary_topic" doc="The switch's secondary topic."/>
  <arg name="output_topic" default="output_topic" doc="The switch's output topic."/>
  <arg name="topic_type" default="float32" doc="The switch's topics' types."/>

  <node name="topic_switch"
        pkg="ezrassor_topic_switch"
        type="topic_switch"
        output="screen">
    <param name="primary_topic" value="$(arg primary_topic)"/>
    <param name="secondary_topic" value="$(arg secondary_topic)"/>
    <param name="output_topic" value="$(arg output_topic)"/>
    <param name="topic_type" value="$(arg topic_type)"/>
  </node>
  -->
</launch>
