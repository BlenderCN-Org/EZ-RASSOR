<?xml version="1.0" encoding="UTF-8"?>

<launch>
  
  <arg name="world" default="mars"/>
  <arg name="gui" default="true" />

  <include file="$(find ezrassor_launcher)/launch/ai_demo.launch">
      <arg name="world" value="$(arg world)"/>
      <arg name="gui" value="$(arg gui)"/>
  </include>

  <node pkg="ez_gui" type="ez_gui" name="ez_gui"/>
  <node pkg="ez_gui" type="monitor" name="monitor"/>

</launch>
