<launch>
  <include file="$(find ezrassor_launcher)/launch/configurable_simulation.launch)">
    <arg name="control_methods" value="app"/>
    <arg name="robot_count" default="4"/>
    <arg name="ports" default="8080 8081 8082 8083"/>
    <arg name="spawn_x_coords" default="1.5 1.5 -1.5 -1.5"/>
    <arg name="spawn_y_coords" default="1.5 -1.5 1.5 -1.5"/>
    <arg name="world" default="ez_battle_bots"/> 
  </include>
</launch>
