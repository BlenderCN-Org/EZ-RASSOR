<?xml version="1.0" encoding="UTF-8"?>
<launch>

   <master auto="start"/>
 <!-- Run the map server --> 
    <node name="map_server" pkg="map_server" type="map_server" args="$(find my_map_package)/my_map.pgm my_map_resolution"/>

 <!--- Run AMCL --> 
    <include file="$(find amcl)/examples/amcl_diff.launch" />

   <node pkg="ai_control" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find ai_control)/local_costmap_config.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find ai_control)/common_costmap_config.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find ai_control)/local_costmap_config.yaml" command="load" />
    <rosparam file="$(find ai_control)/global_costmap_config.yaml" command="load" /> 
    <rosparam file="$(find ai_control)/common_costmap_config.yaml" command="load" />
 </node>

</launch> 